{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "allowJs": true
  },
  "include": ["backend/**/*", "frontend/**/*"],
  "exclude": ["node_modules", "dist", "uploads", "results"]
}